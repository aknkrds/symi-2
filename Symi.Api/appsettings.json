{
  "ConnectionStrings": {
    "Default": "Data Source=symi.db",
    "Redis": "localhost:6379"
  },
  "Jwt": {
    "Issuer": "symi",
    "Audience": "symi-client",
    "AccessTokenSecret": "CHANGE_ME_access_secret_very_long_random",
    "RefreshTokenSecret": "CHANGE_ME_refresh_secret_very_long_random",
    "AccessMinutes": "15",
    "RefreshDays": "14"
  },
  "S3": {
    "Bucket": "symi-dev-bucket"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [ { "Name": "Console" } ]
  }
}
