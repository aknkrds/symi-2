{
  "Logging": {
    "Serilog": {
      "Format": "plain",
      "MinimumLevel": "Debug"
    }
  },
  "Jwt": {
    "Issuer": "symi",
    "Audience": "symi-client",
    "AccessTokenSecret": "dev-access-secret-change-me",
    "RefreshTokenSecret": "dev-refresh-secret-change-me"
  },
  "ConnectionStrings": {
    "Default": "Data Source=symi.db"
  },
  "RateLimit": {
    "DefaultPerMinute": 60,
    "Routes": {
      "/auth/login": 15,
      "/auth/refresh": 30
    }
  }
}
